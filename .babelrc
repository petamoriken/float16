{
    "presets": [["env", {
        "targets": {
            "browsers": [
                "last 2 versions",

                "not IE >= 10",
                "not IE_mob >= 10",
                "Firefox ESR",

                "not OperaMini all",
                "not OperaMobile >= 12.1",

                "not Android >= 4.4.3",
                "not QQAndroid >= 1.2",
                "not UCAndroid >= 11.4",

                "not BlackBerry >= 7",
                "not Baidu >= 7.12",
                "not Samsung >= 4"
            ]
        },
        "exclude": ["transform-regenerator"]
    }]],
    "plugins": [
        ["transform-runtime", {
            "helpers": true,
            "polyfill": false,
            "regenerator": false
        }],
        ["module-resolver", {
            "alias": {
                "lodash-es": "lodash"
            }
        }],
        "add-module-exports"
    ]
}










